#tag EndWebPage

#tag WindowCode
	#tag Event
		Sub Shown()
		  popType.SetFocus
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub HideDescriptions()
		  txtAAffiliate.Visible = False
		  txtAAssociate.Visible = False
		  txtAFull.Visible = False
		  txtAGovernmental.Visible = False
		  txtASpecial.Visible = False
		  txtAStudent.Visible = False
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Function SaveMemType() As Boolean
		  
		  Dim lsStr as String
		  Dim lnLen as Integer
		  Dim oSQL as new cSmartSQL
		  Dim rs as RecordSet
		  
		  
		  'Me.MouseCursor = System.Cursors.Wait
		  
		  if Session.gnRecNo = 0 then
		    oSQL.StatementType = eStatementType.Type_Insert
		  else
		    oSQL.StatementType = eStatementType.Type_Update
		  end
		  
		  oSql.ClearFields
		  oSQL.ClearValues
		  
		  oSQL.AddTable "memapplications"
		  
		  Select Case popDataBookformat.Text
		    
		  Case "CD & Softcover (Additional Cost)"
		    lsStr = "BO"
		  case "CD"
		    lsStr = "CD"
		  Case "Softcover"
		    lsStr = "SC"
		  end
		  
		  
		  oSQL.AddFields "MemType",      "DataBookFormat",            "DonationEducation", "DonationResearch", "DonationSteele"
		  oSQL.AddValues popType.Text, lsStr, chkEducation.Value,   chkResearch.Value,  chkSteele.Value
		  
		  oSQL.AddFields "MemPrice"
		  oSQL.AddValues mdTotalCost
		  
		  if Session.gnRecNo <> 0 then
		    oSQL.AddSimpleWhereClause "memappkwy", Session.gnRecNo
		  else
		    Session.gnRecNo = Session.sesDB.LastID("memapplications")
		  end
		  
		  Session.sesDB.SQLExecute(oSQL.SQL)
		  
		  if Session.gnRecNo = 0 then
		    Session.gnRecNo = Session.sesDB.LastID("memapplications")
		  end
		  
		  
		  if Session.sesDB.CheckDBError then
		    MsgBox(Session.sesDB.ErrorMessage)
		    Return False
		  end
		  
		  frmAppllcation.lblRecNo.Text = "RecNo:" + Str(Session.gnRecNo)
		  
		  Return True
		  
		  
		  
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub UpdateTotal()
		  mdTotalCost = 0
		  app.mbAffiliateGov = False
		  
		  Select Case popType.Text
		  Case "Full"
		    mdTotalCost = App.gdFullMemberPrice
		  case "Associate"
		    mdTotalCost = App.gdAssociateMemberPrice
		  Case "Affiliate"
		    app.mbAffiliateGov = True
		    mdTotalCost = App.gdAffiliateMemberPrice
		  Case "Special"
		    mdTotalCost = App.gdSpecialMemberPrice
		  Case "Governmental"
		    app.mbAffiliateGov = True
		    mdTotalCost = App.gdGovernmental
		  Case "Student"
		    mdTotalCost = App.gdStudentMemberPrice
		    
		  end
		  
		  lblMemshipCost.Text = Format(mdTotalCost, "\$###0.00")
		  
		  Dim ldDataBook as Double
		  
		  If popDataBookformat.Text = "CD & Softcover (Additional Cost)" then
		    ldDataBook = App.gdDataBookBoth
		    lblDatabook.Text = Format(App.gdDataBookBoth, "\$###0.00")
		  else
		    ldDataBook =0
		    lblDatabook.Text = "$0.00"
		  end
		  mdTotalCost = mdTotalCost + ldDataBook
		  
		  Dim ldTotalDonations as Double = 0
		  
		  If chkEducation.Value then
		    ldTotalDonations = ldTotalDonations + App.gdDonationEducation
		  end
		  
		  If chkSteele.Value then
		    ldTotalDonations = ldTotalDonations + App.gdDonationSteele
		  end
		  
		  If chkResearch.Value then
		    ldTotalDonations = ldTotalDonations + App.gdDonationResearch
		  end
		  
		  lblTotalDonations.Text = Format(ldTotalDonations, "\$###0.00")
		  
		  mdTotalCost = mdTotalCost + ldTotalDonations
		  
		  lblGrandTotal.Text = Format(mdTotalCost, "\$###0.00")
		  
		  
		  
		  
		  
		  
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Function ValidateAll() As Boolean
		  Dim lbRetVal as Boolean
		  
		  lbRetVal = True
		  
		  if popType.ListIndex < 0 then
		    popType.Style = EntryFieldsError
		    lbRetVal = False
		  else
		    popType.Style = EntryFields
		  end
		  if popDataBookformat.ListIndex < 0 then
		    popDataBookformat.Style = EntryFieldsError
		    lbRetVal = False
		  else
		    popDataBookformat.Style = EntryFields
		  end
		  Return lbRetVal
		End Function
	#tag EndMethod


	#tag Property, Flags = &h0
		mdTotalCost As Integer = 0
	#tag EndProperty


#tag EndWindowCode

#tag Events popDataBookformat
	#tag Event
		Sub LostFocus()
		  if me.ListIndex < 0 then
		    me.Style = EntryFieldsError
		  else
		    me.Style = EntryFields
		  end
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub SelectionChanged()
		  UpdateTotal
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events popType
	#tag Event
		Sub SelectionChanged()
		  HideDescriptions
		  app.mbAffiliateGov = False
		  
		  
		  txtADirection.Visible = False
		  Select Case popType.Text
		    
		  Case "Full"
		    txtAFull.Visible = True
		    lblMemberType.Text = "Member Type: Full"
		  Case "Affiliate"
		    txtAAffiliate.Visible = True
		    lblMemberType.Text = "Member Type: Affiliate"
		    app.mbAffiliateGov = True
		  Case "Associate"
		    txtAAssociate.Visible = True
		    lblMemberType.Text = "Member Type: Associate"
		  Case "Governmental"
		    txtAGovernmental.Visible = True
		    lblMemberType.Text = "Member Type: Governmental"
		    app.mbAffiliateGov = True
		  Case "Special"
		    txtASpecial.Visible = True
		    lblMemberType.Text = "Member Type: Special"
		  Case "Student"
		    txtAStudent.Visible = True
		    lblMemberType.Text = "Member Type: Student"
		  end
		  
		  lblMemberType.Text = "Member Type selected " + popType.Text + ":"
		  
		  UpdateTotal
		End Sub
	#tag EndEvent
	#tag Event
		Sub LostFocus()
		  if me.ListIndex < 0 then
		    me.Style = EntryFieldsError
		  else
		    me.Style = EntryFields
		  end
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events chkSteele
	#tag Event
		Sub ValueChanged()
		  UpdateTotal
		End Sub
	#tag EndEvent
